{
    // does not render single space between words"editor.renderWhitespace": "boundary",
    // vertical rule after 80 charachter"editor.rulers": [80],
    // turns off validations that VSCode"javascript.validate.enable": false,
    // tab = 2 spaces"editor.tabSize": 2,
    // allow prettier to fix eslint // "eslint.autoFixOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  }